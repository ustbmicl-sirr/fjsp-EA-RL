{
    "title": "FJSSP schema",
    "type": "object",
    "properties": {
        "sp_type": {
            "type": "string"
        },
        "instances_file": {
            "type": "string"
        },
        "seed": {
            "type": "integer",
            "minimum": 0
        },
        "num_jobs": {
            "type": "integer",
            "minimum": 1
        },
        "num_tasks": {
            "type": "integer",
            "minimum": 1
        },
        "num_machines": {
            "type": "integer",
            "minimum": 1
        },
        "num_tools": {
            "type": "integer",
            "minimum": 0
        },
        "runtimes": {
            "type": "array",
            "items": {
                "type": "integer"
            }
        },
		"num_instances": {
            "type": "integer",
            "minimum": 1
        },
		"num_processes": {
            "type": "integer",
            "minimum": 1
        },
		"write_to_file": {
            "type": "boolean"
        }
    },
    "required": [
        "sp_type",
        "num_jobs",
		"num_tasks",
		"num_machines",
		"num_instances"
    ]
}
